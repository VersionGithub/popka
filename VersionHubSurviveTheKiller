local currentFOV = workspace.CurrentCamera.FieldOfView

local function changeFOV(value)
    currentFOV = value
    workspace.CurrentCamera.FieldOfView = currentFOV
end

Tab:AddSlider({
    Name = "Field of View",
    Min = 70,
    Max = 120,
    Default = currentFOV,
    Color = Color3.fromRGB(255, 255, 255),
    Increment = 1,
    ValueName = "FOV",
    Callback = function(value)
        changeFOV(value)
    end    
})

local InfiniteJumpEnabled = false

Tab:AddToggle({
    Name = "Бесконечный прыжок",
    Default = false,
    Callback = function(Value)
        InfiniteJumpEnabled = Value
        print("Бесконечный прыжок:", Value)
    end    
})

game:GetService("UserInputService").JumpRequest:connect(function()
    if InfiniteJumpEnabled then
        local player = game:GetService("Players").LocalPlayer
        local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid:ChangeState("Jumping")
        end
    end
end)

-- Переменные для отслеживания состояния Noclip
local NoclipEnabled = false
local Noclip = nil
local Clip = nil

-- Функция для включения Noclip
function noclip()
    Clip = false
    local function Nocl()
        if Clip == false and game.Players.LocalPlayer.Character ~= nil then
            for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if v:IsA('BasePart') and v.CanCollide then
                    v.CanCollide = false
                end
            end
        end
        wait(0.21) -- basic optimization
    end
    Noclip = game:GetService('RunService').Stepped:Connect(Nocl)
end

-- Функция для выключения Noclip
function clip()
    if Noclip then Noclip:Disconnect() end
    Clip = true
    if game.Players.LocalPlayer.Character ~= nil then
        for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if v:IsA('BasePart') and not v.CanCollide then
                v.CanCollide = true
            end
        end
    end
end

-- Добавьте чекбокс (Checkbox)
Tab:AddToggle({
    Name = "Noclip",
    Default = false,
    Callback = function(Value)
        NoclipEnabled = Value
        if NoclipEnabled then
            noclip()
        else
            clip()
        end
        print("Noclip:", Value)
    end    
})

